{
	"name": "boop",
	"displayName": "Boop!",
	"description": "Syntax highlighting for Boop! build scripts",
	"icon": "icon@2x.png",
	"version": "1.2.0",
	"publisher": "ProPuke",
	"license": "MIT",
	"engines": {
		"vscode": "^1.66.0"
	},
	"categories": [
		"Programming Languages"
	],
	"repository": {
		"type": "git",
		"url": "https://github.com/ProPuke/vscode-boop.git"
	},
	"keywords": [
		"boop",
		"build system"
	],
	"contributes": {
		"languages": [
			{
				"id": "boop",
				"aliases": [
					"Boop",
					"boop"
				],
				"extensions": [
					".boop"
				],
				"configuration": "./language-configuration.json"
			}
		],
		"grammars": [
			{
				"language": "boop",
				"scopeName": "source.boop",
				"path": "./syntaxes/boop.tmLanguage.json"
			}
		]
	},
	"scripts": {
		"package": "vsce package"
	},
	"devDependencies": {
		"vsce": "^2.7.0"
	}
}
